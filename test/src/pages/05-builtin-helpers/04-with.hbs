{{#with builtin.person}}
	{{firstname}} {{lastname}}
{{/with}}

{{#with builtin.city as | city |}}
	{{#with city.location as | loc |}}
		{{city.name}}: {{loc.north}} {{loc.east}}
	{{/with}}
{{/with}}

{{#with builtin.city2}}
	{{city.name}} (not shown because there is no city)
{{else}}
	No city found
{{/with}}
