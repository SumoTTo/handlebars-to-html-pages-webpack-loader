	"dependencies": {
		"front-matter": "*",
		"glob": "*",
		"handlebars": "*",
		"js-yaml": "*",
		"schema-utils": "*"
	},
	"devDependencies": {
		"@babel/core": "^7",
		"@babel/eslint-parser": "^7",
		"@babel/plugin-proposal-class-properties": "^7",
		"@babel/plugin-proposal-private-methods": "^7",
		"@babel/preset-env": "^7",
		"@babel/register": "^7",
		"@sumotto/multiple-html-pages-webpack-plugin": "^1",
		"@wordpress/eslint-plugin": "latest",
		"cross-env": "latest",
		"eslint": "latest",
		"html-loader": "^3",
		"html-webpack-plugin": "^5",
		"husky": "latest",
		"jest": "latest",
		"np": "latest",
		"terser": "latest"
	},
	"peerDependencies": {
		"webpack": ">=5"
